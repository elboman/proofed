{
  "name": "proofed",
  "version": "0.1.0",
  "description": "A flexible library for user interaction validation",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "start": "NODE_ENV=development webpack-dev-server --host 0.0.0.0 --port 8000 --config webpack.config.js --history-api-fallback --content-base example",
    "bundle": "rimraf _bundles && mkdirp _bundles && NODE_ENV=production webpack",
    "compile": "rimraf dist && mkdirp dist && node ./node_modules/typescript/bin/tsc",
    "package": "npm run bundle && npm run compile"
  },
  "author": {
    "name": "Marco Botto",
    "web": "https://github.com/elboman"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/elboman/proofed.git"
  },
  "license": "MIT",
  "devDependencies": {
    "awesome-typescript-loader": "^1.1.1",
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "rimraf": "^2.5.4",
    "ts-loader": "^0.8.2",
    "tsc": "^1.20150623.0",
    "typescript": "^1.8.10",
    "typings": "^1.3.2",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "lodash": "^4.14.2"
  }
}
